  },
  "private": true,
  "license": "UNLICENSED",
  "husky": {},
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prepare": "husky",
    "verify": "pnpm tsc --noEmit",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" ",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "vitest run -c vitest.unit.config.mts",
    "test:watch": "vitest -c vitest.unit.config.mts",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run -c ./vitest.e2e.config.mts",
    "test:e2e:watch": "vitest  -c ./vitest.e2e.config.mts",
    "test:create-prisma-environment": "pnpm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "pnpm add vitest-environment-prisma",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment"
  },
  "dependencies": {
    "@anatine/zod-nestjs": "^2.0.9",
    "@anatine/zod-openapi": "^2.2.6",
    "@aws-sdk/client-s3": "^3.658.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.19.1",
    "@types/passport-jwt": "^4.0.1",
    "D": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.13",
    "faker-br": "^0.4.1",
    "module": "^1.2.5",
    "openapi3-ts": "^4.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "vitest-environment-prisma": "^0.4.2",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.11.0",
    "@eslint/migrate-config": "^1.3.0",
    "@faker-js/faker": "^9.0.2",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@rocketseat/eslint-config": "^2.2.2",
    "@swc/core": "^1.7.26",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitest/coverage-v8": "^2.1.1",
    "dochelper": "^1.1.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.19.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "unplugin-swc": "^1.5.1",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.1"
  }
}
